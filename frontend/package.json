{
  "name": "neolace-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "predev": "npm install && npm run compile:i18n && cd ../neolace-api && ./_compile.ts && cd - && node ./copy-plugin-public-files.mjs",
    "dev": "next dev -p 5555",
    "prebuild": "node ./copy-plugin-public-files.mjs",
    "build": "next build",
    "lint": "next lint",
    "start": "next start -p 5555",
    "entrypoint-dev": "npm link /local-dependencies/technotes-mdt/ && npm install && npm run dev",
    "extract:i18n": "formatjs extract '{pages,components,sections,plugins}/**/*.{js,ts,tsx}' --format simple --out-file content/locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n",
    "test": "jest --watch"
  },
  "dependencies": {
    "@antv/g6": "4.7.4",
    "@popperjs/core": "2.11.6",
    "blurhash": "2.0.3",
    "graphology": "0.25.0",
    "graphology-communities-louvain": "2.0.1",
    "graphology-metrics": "2.1.0",
    "graphology-operators": "1.6.0",
    "graphology-types": "0.24.4",
    "neolace-api": "file:../neolace-api/dist/",
    "neolace-search-frontend": "file:./plugins/search/",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-blurhash": "0.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.1.2",
    "react-popper": "2.3.0",
    "slate": "0.82.1",
    "slate-history": "0.66.0",
    "slate-react": "0.83.1",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@formatjs/cli": "5.1.1",
    "@next/bundle-analyzer": "12.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/node": "18.8.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "10.4.12",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.0",
    "eslint-plugin-formatjs": "4.3.2",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "postcss": "8.4.17",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.3"
  }
}
